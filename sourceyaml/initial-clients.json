{
    "tenants": [
      {
        "Name": "Siccar.Net",
        "Authority": "https://login.microsoftonline.com/{{APPTENANTID}}/",
        "ClientId": "{{APPCLIENTID}}",
        "CallbackPath": "/signin-ad",
        "SignedOutCallbackPath": "/signout-callback-ad",
        "RemoteSignOutPath": "/signout-ad",
        "ResponseType": "id_token",
        "TokenValidationParameters": {
          "AuthenticationType":  "oidc",
          "NameClaimType": "name",
          "RoleClaimType": "role"
        },
        "AdminEmail": "{{EMAILID}}"
      }
    ],
    "clients": [
      {
        "ClientId": "siccar-admin-ui-client",
        "RequireClientSecret": false,
        "AllowedGrantTypes": [
          "authorization_code",
          "client_credentials"
        ],
        "RedirectUris": [
          "https://{{TENANTISSUER}}/admin/authentication/login-callback"
        ],
        "PostLogoutRedirectUris": [
          "https://{{TENANTISSUER}}/admin/signout-callback-oidc"
        ],
        "AllowOfflineAccess": true,
        "AllowedCorsOrigins": [
          "https://{{TENANTISSUER}}"
        ],
        "AllowedScopes": [
          "openid",
          "profile",
          "api",
          "wallet.read",
          "wallet.update",
          "register.admin",
          "blueprint.publish",
          "wallet.admin",
          "tenant.admin",
          "installation.admin"
        ]
      }
    ]
  }